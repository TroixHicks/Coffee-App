<!doctype html>
<html>
<head>
    <title>Coffee App</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Zen+Kurenaido&display=swap" rel="stylesheet">  
</head>
<body>
<div class="container">

    <div class="jumbotron text-center">
        <h1>Dolce Caffe</h1>
        <div class="title">
            

            <input id="name" type="text" placeholder="Customers Name">
        </div>
        
        
        <section class="top">
            <h2 class="menu"> Menu</h2>
            <button id="icedCoffee" data-id='Iced coffee'>Iced Coffee</button>
            <button id="hotCoffee" data-id='Hot coffee'>Hot Coffee</button>
            <button id="latte" data-id='Latte'>Latte</button>
            <button id="machhiato" data-id='Latte'>Machhiato</button>
            <button id="expresso" data-id='Expresso'>Expresso</button>
            <button id="cappuccino" data-id='cappuccino'>Cappuccino</button>
        </section>

       

        
        <h2> Customer Queue</h2>
        <span> </span>

        <p>Login or Signup:</p>

        <a href="/login" class="btn btn-default"><span class="fa fa-user"></span>Login</a>
        <a href="/signup" class="btn btn-default"><span class="fa fa-user"></span>Signup</a>

    </div>
</div>
<script src="main.js"></script>
</body>
</html>
